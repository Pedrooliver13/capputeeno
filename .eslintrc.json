{
  "extends": "next/core-web-vitals",
  "plugins": ["grouped-import"],
  "rules": {
    "@typescript-eslint/no-empty-interface": 0,
    "grouped-import/group": [
      "warn",
      {
        "Packages": [
          { "path": "react" },
          { "path": "next" },
          { "path": "immer" },
          { "path": "styled-components" },
          { "path": "@hookform" },
          { "path": "zod" },
          { "path": "phosphor-react" },
          { "path": "@emotion" },
          { "path": "react-toastify" }
        ],
        "Libs": [{ "path": "lib" }],
        "Templates": [{ "path": "templates" }],
        "Pages": [{ "path": "pages/" }],
        "Reducers": [{ "path": "reducer" }, { "path": "action" }],
        "Components": [{ "path": "components" }, { "path": "./components" }],
        "Contexts": [{ "path": "contexts" }],
        "Hooks": [{ "path": "hooks" }],
        "Utils": [{ "path": "utils" }],
        "Styles": [
          { "path": "styles" },
          { "path": ".css" },
          { "path": ".styles" }
        ],
        "Assets": [
          { "path": "assets" },
          { "path": ".png" },
          { "path": ".svg" },
          { "path": ".jpg" },
          { "path": ".ttf" }
        ]
      }
    ]
  }
}
